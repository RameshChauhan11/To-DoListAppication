@model ICollection<SignInModel>;
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<html>
<head>
</head>
<body>
    <div class="container">
        <div class="col-sm-12">
            <table class="table table-border text-center" border="2">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Mobile</th>
                        <th>Password</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null)
                    {
                        foreach (var data in Model)
                        {
                            <tr>
                                <td>@data.Id</td>
                                <td>@data.name</td>
                                <td>@data.email</td>
                                <td>@data.mobile</td>
                                <td>@data.password</td>
                                <td>
                                    <a class="btn btn-outline-danger" href="/Admin/DeleteSignRecord/@data.Id">Delete</a>&nbsp;

                                </td>
                            </tr>
                        }
                    }
                    else
                    {
                        <p>No data present </p>
                    }

                </tbody>
            </table>


        </div>
    </div>
</body>
</html>
